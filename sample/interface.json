{
    "name": "MyDemo3", // 项目名。用户 GUI 称呼当前资源
    "title": "Demo3 v1.0.0", // 窗口标题，给什么就显示什么，UI 不要往上加别的东西。默认 null，则使用 name 和 version 拼接
    "icon": "demo3.png", // 应用图标，若不指定，则使用默认的图标
    "mirrorchyan_rid": "demo3",
    "mirrorchyan_multiplatform": false,
    "auto_update_ui": true, // 是否支持自动更新 UI，若为 false，则仅跟随资源发布更新 UI，由资源作者自行在 CI 中指定 UI 版本。UI 上无 UI 的更新选项，用户无法自行更新
    "auto_update_maafw": true, // 是否支持自动更新 MaaFramework，若为 false，则仅跟随资源发布更新 MaaFramework，由资源作者自行在 CI 中指定 MaaFramework 版本。UI 上无 MaaFramework 的更新选项，用户无法自行更新
    "github_owner": "MAA1999", // GitHub 资源作者
    "github_repo": "M9A", // GitHub 资源仓库
    "github_mirrors": [], // GitHub 镜像仓库列表，若不指定，则使用默认的 GitHub 镜像仓库
    "version": "1.0.0",
    "contact_us": {    // 这里直接 key value 铺在 设置-关于 里面
        "QQ群": "123456789", // QQ 群号
        "QQ群链接": "https://qm.qq.com/cgi-bin/qm/qr?k=xxxxxxxxxxxxxxxxxxxxxx", // QQ 群链接
        "B站": "https://space.bilibili.com/12345678", // B站空间链接
    },
    "description": "Welecome to MaaFramework!", // 描述应支持 markdown 和 html
    "agent": {
        "child_exec": "python",
        "child_args": [
            "sample/python/demo3_agent.py"
        ]
    },
    "controller": [
        {
            "name": "安卓端",
            "description": "通过 ADB 连接安卓设备", // 描述，应支持 markdown 和 html
            "icon": "adb.png", // 图标，可选
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "Official",
            "description": "官服资源", // 描述，应支持 markdown 和 html
            "icon": "official.png", // 图标，可选
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "Bilibili",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "收取荒原",
            "entry": "Wilderness",
            "force_check": false, // 是否强制选中
            "default_check": true, // 是否默认选中
            "description": "收取荒原资源，若不勾选，则不会执行此任务", // 任务详细描述，应支持 markdown 和 html
            "icon": "wilderness.png" // 任务图标，给用户看的
        },
        {
            "name": "每日心相（意志解析）",
            "entry": "Psychube"
        },
        {
            "name": "常规作战",
            "entry": "Combat",
            "option": [
                "作战关卡",
                "复现次数",
                "刷完全部体力"
            ]
        },
        {
            "name": "活动：绿湖噩梦 17 艰难（活动已结束）",
            "entry": "ANightmareAtGreenLake",
            "option": [
                "复现次数",
                "刷完全部体力"
            ],
            "pipeline_override": {
                "EnterTheShow": {
                    "next": "ANightmareAtGreenLake"
                },
                "TargetStageName": {
                    "expected": "17"
                },
                "StageDifficulty": {
                    "next": "ActivityStageDifficulty"
                }
            }
        },
        {
            "name": "领取奖励",
            "entry": "Awards"
        }
    ],
    "option": {
        "作战关卡": {
            "cases": [
                {
                    "name": "3-9 厄险（百灵百验鸟）",
                    "description": "刷百灵鸟", // 描述，应支持 markdown 和 html
                    "icon": "百灵鸟.png", // 图标，可选
                    "pipeline_override": {
                        "EnterTheShow": {
                            "next": "MainChapter_3"
                        },
                        "TargetStageName": {
                            "expected": "09"
                        },
                        "StageDifficulty": {
                            "next": "StageDifficulty_Hard"
                        }
                    }
                },
                {
                    "name": "4-20 厄险（双头形骨架）",
                    "pipeline_override": {
                        "EnterTheShow": {
                            "next": "MainChapter_4"
                        },
                        "TargetStageName": {
                            "expected": "20"
                        },
                        "StageDifficulty": {
                            "next": "StageDifficulty_Hard"
                        }
                    }
                },
                {
                    // 若 case 中仅有一个 input 的选项，考虑不显示 checkbox，而是直接显示输入框
                    "name": "自定义关卡",
                    "description": "自己选打什么关", // 描述，应支持 markdown 和 html
                    "icon": "扳手.png", // 图标，可选
                    "input": [
                        {
                            "name": "章节号",
                            "type": "number", // 用户输入类型校验
                            "default": 4
                        },
                        {
                            "name": "关卡号",
                            "type": "number" // 若没有 default，则用户必须输入
                        },
                        {
                            "name": "难度",
                            "type": "string",
                            "default": "Hard",
                            "verify": "^(Normal|Hard)$" // 正则表达式校验用户输入
                        }
                    ],
                    "pipeline_override": {
                        "EnterTheShow": {
                            "next": "MainChapter_{章节号}"
                        },
                        "TargetStageName": {
                            "expected": "{关卡号}"
                        },
                        "StageDifficulty": {
                            "next": "StageDifficulty_{难度}"
                        }
                    }
                }
            ]
        },
        "复现次数": {
            "description": "打几次", // 描述，应支持 markdown 和 html
            "icon": "times.png", // 图标，可选
            "cases": [
                {
                    "name": "x1",
                    "description": "打1次", // 描述，应支持 markdown 和 html
                    "icon": "once.png", // 图标，可选
                    "pipeline_override": {
                        "SetReplaysTimes": {
                            "expected": "1"
                        }
                    }
                },
                {
                    "name": "x2",
                    "pipeline_override": {
                        "SetReplaysTimes": {
                            "expected": "2"
                        }
                    }
                },
                {
                    "name": "x3",
                    "pipeline_override": {
                        "SetReplaysTimes": {
                            "expected": "3"
                        }
                    }
                },
                {
                    "name": "x4",
                    "pipeline_override": {
                        "SetReplaysTimes": {
                            "expected": "4"
                        }
                    }
                }
            ]
        },
        "刷完全部体力": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "AllIn": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "AllIn": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}